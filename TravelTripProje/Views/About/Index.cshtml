@using TravelTripProje.Models.Siniflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var item in Model)
            {
                <div align="center"><img src="@item.FotoUrl" width="850" alt="" /></div>
                <p>@item.Aciklama</p>   
            }
        </div>
    </div>
</div>